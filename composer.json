{
  "name": "suframe/think-admin",
  "description": "suframe thinkphp admin",
  "license": "Apache-2.0",
  "authors": [
    {
      "name": "qian",
      "email": "330576744@qq.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "suframe\\thinkAdmin\\": "src"
    },
    "files": [
      "src/helpers.php"
    ]
  },
  "extra": {
    "think": {
      "services": [
        "suframe\\thinkAdmin\\AdminService"
      ],
      "config": {
        "thinkAdmin": "src/config/thinkAdmin.php"
      }
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "topthink/think-view": "^1.0",
    "suframe/form": "^1.0",
    "myclabs/php-enum": "^1.7",
    "topthink/think-migration": "^3.0"
  },
  "require-dev": {
    "swoole/ide-helper": "@dev"
  }
}